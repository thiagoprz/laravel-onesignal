{
  "name": "thiagoprz/laravel-onesignal",
  "description": "One Signal Laravel service",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "dev",
  "extra": {
    "laravel": {
      "providers": [
        "OnesignalServiceProvider"
      ]
    }
  },
  "autoload": {
    "classmap": [
      "src/"
    ],
    "psr-4": {
      "Thiagoprz\\Onesignal\\": "src"
    }
  },
  "autoload-dev": {
    "Thiagoprz\\Onesignal\\Tests\\": "tests",
    "classmap": [
      "tests"
    ]
  },
  "require": {
    "php": "8.1",
    "laravel/framework": "8.x-dev",
    "guzzlehttp/guzzle": "^7.0",
    "ext-json": "*",
    "spatie/data-transfer-object": "^3.7.3"
  },
  "require-dev": {
    "orchestra/testbench": "6"
  }
}